{
  "mcpServers": {
    "Parallel Task MCP": {
      "type": "http",
      "url": "https://task-mcp.parallel.ai/mcp",
      "timeout": 60000,
      "retries": 3,
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    "Parallel Search MCP": {
      "type": "http",
      "url": "https://search-mcp.parallel.ai/mcp",
      "timeout": 60000,
      "retries": 3,
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    "mcp-image": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-image"],
      "env": {
        "GEMINI_API_KEY": "${GEMINI_API_KEY}",
        "IMAGE_OUTPUT_DIR": "/path/to/article_images"
      }
    }
  }
}

